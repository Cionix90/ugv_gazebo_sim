    <xacro:macro name="wheels_collision" params=" 
    mu
    slip
    ">
        <surface>
            <friction>
                <ode>
                    <mu>
                        ${mu}
                    </mu>
                    <mu2>
                        0.0
                    </mu2>
                    <slip1>
                        ${slip}
                    </slip1>
                    <slip2>
                        0.0
                    </slip2>

                </ode>
            </friction>
        </surface>
        <contact>
            <ode>
                <kp>10000</kp>
                <kd>1</kd>
            </ode>
        </contact>
    </xacro:macro>

